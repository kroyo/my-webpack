{
  "name": "vue-webpack",
  "version": "1.0.0",
  "description": "studing webpack and vue ",
  "main": "index.js",
  "repository": "https://github.com/kroyo/vue-webpack.git",
  "author": "kroyo <kroyoshi@163.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
